#SHCMD
super + shift + Return
	alacritty
super + space
	rofi -show drun -show-icons
super + shift + n
	thunar
super + shift + f
	firefox
super + n
	nnn
#AUDIO
#pulseaudio -
super + shift + F1
	pactl set-sink-volume alsa_output.pci-0000_00_05.0.analog-stereo -5%
#pulseaudio +
super + shift + F2
	pactl set-sink-volume alsa_output.pci-0000_00_05.0.analog-stereo +5%
super + shift + F3
	amixer set Master mute
super + shift + F4
	amixer set Master unmute
#RELOAD
super + shift + q
	bspc quit & killall polybar
super + shift + r
	bspc wm -r
super + ctrl + r
	pkill -USR1 sxhkd
#SCREENSHOTS
Print	
	shutter -s
