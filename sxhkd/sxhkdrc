#WINDOW_MANAGMENT
super + shift + c
	bspc  node -c
super + h
	bspc node -z left -20 0
super + k
	bspc node -z top 0 -20
super + shift + h
	bspc node -z left 20 0
super + shift + k
	bspc node -z top 0 20
super + o
	bspc node -f west
super + p
	bspc node -f east
super + shift + o
	bspc node -f north
super + shift + p
	bspc node -f south
super + f
	bspc node -t floating
super + t
	bspc node -t tiled
